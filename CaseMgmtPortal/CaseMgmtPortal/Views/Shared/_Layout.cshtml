                    </h1>
                </a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#NavigationIcons" aria-controls="NavigationIcons" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="fas fa-bars mr-1" aria-hidden="true"></span>Menu
                </button>
                <div class="collapse navbar-collapse" id="NavigationIcons">
                    <ul class="navbar-nav navbar-icon-links">
                        <li class="nav-item">
                            <a class="nav-link" href="/Home"><span class="fas fa-home" aria-hidden="true"></span><span class="">Reporting Child Neglect</span></a>
                            <div class="icon-link-border-bottom"></div>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link active" href="/CreateCase"><span class="fas fa-solid fa-file-alt" aria-hidden="true"></span><span class="">Submit New Report</span></a>
                            <div class="icon-link-border-bottom"></div>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="/ViewCases"><span class="fas fa-solid fa-eye" aria-hidden="true"></span><span class="">View Submitted Cases</span></a>
                            <div class="icon-link-border-bottom"></div>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
        <main role="main">
            @RenderBody()
        </main>
    <a id="ScrollToTop" href="#main" class="btn-scroll-top" role="button">
        <span class="fas fa-arrow-up" aria-hidden="true"></span>
        <span class="sr-only">Go back to the top of this page</span>
    </a>

    <footer class="footer footer-basic fix">
        <div class="container-fluid" role="navigation" aria-label="Footer navigation" id="FooterNavigation">
            <div class="links-wrapper" role="navigation">
                <a class="btn btn-link" href="/Home">Home</a>
                <a class="btn btn-link" href="Account">Help</a>
                <a class="btn btn-link" href="MoreInfo">More Info</a>
                <a class="btn btn-link" href="Home\Index">Privacy</a>
            </div>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/pelican/dist/js/pelican.bundle.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>